Create database tarea1;
Create table Usuario

( idUsuario smallint not null auto_increment,
nombre varchar(30),
apellidoPat varchar(25),
apellidoMat varchar(25),
Usuario		varchar(10),
Contraseña	varchar(10),
primary key (idUsuario)
);
